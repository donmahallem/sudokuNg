{
  "name": "@donmahallem/sudoku-ng",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:unit": "npm run test:ci",
    "test:build": "ng build --prod --aot --base-href ./",
    "test:lint": "ng lint",
    "test:docs": "npm run compodoc",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -d ./docs",
    "postinstall": "npm run build:pug",
    "prepublishOnly": "npm run test && npm run docs && npm run build",
    "build:githubpages": "ng build --aot --prod --base-href=/sudokuNg/ --deploy-url=/sudokuNg/",
    "build:ssr:github": "npm run build:github-client-and-server-bundles && npm run compile:server",
    "build:github-client-and-server-bundles": "npm run build:githubpages && ng run sudokuNg:server:production --bundleDependencies all",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node dist/server",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run sudokuNg:server:production --bundleDependencies all",
    "dev:ssr": "ng run sudokuNg:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run sudokuNg:server:production",
    "prerender": "ng run sudokuNg:prerender",
    "prerender:githubpages": "ng run sudokuNg:prerender --configuration=production,githubpages",
    "build:pug": "pug ./src --doctype html"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://donmahallem.github.io/sudokuNg/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/donmahallem/sudokuNg/issues"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donmahallem/sudokuNg.git"
  },
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/cdk": "~9.2.2",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/material": "~9.2.2",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/platform-server": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@angular/service-worker": "~9.1.3",
    "@nguniversal/express-engine": "~9.1.0",
    "core-js": "~3.6.5",
    "express": "~4.17.1",
    "material-design-icons": "~3.0.1",
    "rxjs": "~6.5.5",
    "tslib": "~1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.6",
    "@compodoc/compodoc": "~1.1.10",
    "@nguniversal/builders": "~9.1.0",
    "@types/express": "~4.17.6",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.13.4",
    "apply-loader": "~2.0.0",
    "codelyzer": "~5.2.2",
    "commitizen": "~4.1.2",
    "cz-conventional-changelog": "~3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.5.3",
    "protractor": "~5.4.4",
    "pug": "~2.0.4",
    "pug-cli": "~1.0.0-alpha6",
    "pug-loader": "~2.4.0",
    "ts-loader": "~7.0.2",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3",
    "webpack-cli": "~3.3.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
