{
  "name": "@donmahallem/sudoku-ng",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:unit": "npm run test:ci",
    "test:build": "ng build --prod --aot --base-href ./",
    "test:lint": "ng lint",
    "test:docs": "npm run compodoc",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -d ./docs",
    "postinstall": "node ./ng-add-pug-loader.js",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:githubpages": "ng build --aot --prod --base-href=/sudokuNg/ --deploy-url=/sudokuNg/",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:ssr:github": "npm run build:github-client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run sudokuNg:server:production --bundleDependencies all",
    "build:github-client-and-server-bundles": "npm run build:githubpages && ng run sudokuNg:server:production --bundleDependencies all"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://donmahallem.github.io/sudokuNg/",
  "author": {
    "name": "donmahallem",
    "email": "donmahallem@users.noreply.github.com",
    "url": "https://github.com/donmahallem"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/donmahallem/sudokuNg/issues"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donmahallem/sudokuNg.git"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.10",
    "@angular/router": "~8.2.14",
    "@nguniversal/express-engine": "~8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "~8.2.6",
    "core-js": "~3.6.4",
    "express": "~4.17.1",
    "material-design-icons": "^3.0.1",
    "ng-cli-pug-loader": "~0.2.0",
    "rxjs": "~6.5.3",
    "tslib": "~1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~9.0.2",
    "@compodoc/compodoc": "~1.1.10",
    "@types/express": "~4.17.0",
    "@types/jasmine": "~3.5.5",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.7.4",
    "apply-loader": "~2.0.0",
    "codelyzer": "~5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.5.2",
    "protractor": "~5.4.3",
    "pug": "~2.0.4",
    "pug-loader": "~2.4.0",
    "ts-loader": "~6.2.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "<3.6.0",
    "webpack-cli": "~3.3.11"
  }
}
